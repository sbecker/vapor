./script/generate rspec_scaffold instance \
  account_id:integer \
  ami_launch_index:integer \
  aws_availability_zone:string \
  aws_groups:string \
  aws_image_id:string \
  aws_instance_id:string \
  aws_instance_type:string \
  aws_kernel_id:string \
  aws_launch_time:datetime \
  aws_owner:string \
  aws_product_codes:string \
  aws_ramdisk_id:string \
  aws_reason:string \
  aws_reservation_id:string \
  aws_state:string \
  aws_state_code:string \
  dns_name:string \
  private_dns_name:string \
  ssh_key_name:string

./script/generate rspec_scaffold snapshot \
  account_id:integer \
  aws_id:string \
  aws_progress:string \
  aws_started_at:datetime \
  aws_status:string \
  aws_volume_id:string


